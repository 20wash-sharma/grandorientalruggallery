	color:#000000;
}
#prd li a:hover{
	color:#996600;
}
.hl{
	display:block;
	float:left;
	width:auto;
	padding:5px 5px 5px 25px;
	background-color:#FFFFCC !important;
	border:dashed #FF9900 1px;
	color:#660000 !important;
	background:url(imgs/alert.png) no-repeat 5px 8px;
}
.ph{
	display:block;
	float:left;
	width:auto;
	padding:5px 5px 5px 25px;
	background-color:#EEEEEE !important;
	border:dashed #C8C8C8 1px;
	color:#555555 !important;
	background:url(imgs/phone.png) no-repeat 5px 8px;
}
.pic{
	float:left;
	padding:5px;
	border:solid #C8C8C8 1px;
	margin:0px 10px 10px 0px;
}
.sm{
	font-size:11px;
}
.tbl_rugs table{
	font-size:11px;
