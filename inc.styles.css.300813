}
#prd, #prd li{
	list-style:none;
	display:inline;
	margin:0px;
	padding:0px;
}
#prd li a{
	display:block;
	float:left;
	width:400px;
	padding:10px 5px 10px 40px;
	background:url(imgs/rug-bullet.png) no-repeat 10px 12px;
	border-bottom:dashed #C8C8C8 1px;
